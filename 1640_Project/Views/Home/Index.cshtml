@model List<_1640_Project.Models.Submission>

@{
    ViewBag.Title = "Submit";
}

<link href="~/Styles/HomePage.css" rel="stylesheet" />

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>HomePage</title>
</head>

<body>
    <div class="banner">
        <div class="content">
            <h1>Submit Your Idea</h1>
            <p>Choose your industry</p>
            <div>
                @foreach (var item in Model)
                {
                <a href="/home/category/@item.SubmissionID"><button type="button"><span></span>@item.SubmissionName</button></a>
                }
            </div>
        </div>
    </div>

</body>
</html>




